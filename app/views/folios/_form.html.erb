<%= form_for @folio_item do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />

  <%= f.label :subtitle %>:
  <%= f.text_field :subtitle %><br />

  <%= f.label :body %>:
  <%= f.text_field :body %><br />
  <ul>
  <%= f.fields_for :technologies do |tech| %>

    <%= tech.label :name %>:
    <%= tech.text_field :name %><br>

  <% end %>
  </ul>
  

  <%= f.submit %>
<% end %>
